<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="624.0"
            xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
            fx:controller="sample.controller.Controller">
    <children>
        <!-- Таблица в которую мы будем выводит наш pojo -->
        <TableView fx:id="tableUsers" layoutX="12.0" layoutY="62.0" prefHeight="324.0" prefWidth="1200.0">
            <columns>
                <!-- Колонки таблицы -->
                <TableColumn prefWidth="150.0" text="Name" fx:id="nameColumn" />
                <TableColumn prefWidth="150.0" text="Adress" fx:id="adressColumn" />
                <TableColumn prefWidth="150.0" text="Number" fx:id="numberColumn" />
                <TableColumn prefWidth="220.0" text="Specialization" fx:id="specializationColumn" />
                <TableColumn prefWidth="150.0" text="Day" fx:id="dayColumn" />
                <TableColumn prefWidth="150.0" text="Time" fx:id="timeColumn" />
                <TableColumn prefWidth="150.0" text="Str" fx:id="strColumn" />
            </columns>
        </TableView>
        <Label layoutX="14.0" layoutY="18.0" text="List all Trade Points:">
            <font>
                <Font name="Verdana" size="24.0" />
            </font>
        </Label>
    </children>
</AnchorPane>